{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": false
    }
  },
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["@nuxt/types", "@nuxtjs/axios", "@types/node", "@/types/*.d.ts"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
